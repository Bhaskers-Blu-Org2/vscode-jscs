{
	"name": "jscs",
	"description": "jscs",
	"version": "0.0.1",
	"publisher": "vscode",
	"engines": {
		"vscode": "*"
	},
	"activationEvents": [
		"onLanguage:javascript",
		"command:extension.sayHello"
	],
	"main": "./out/extension",
	"contributes": {
		"commands"
			: [{
				"command": "extension.sayHello",
				"title": "jscs linter command"
			}
		],
		"configuration": {
			"type": "object",
			"title": "jscs configuration",
			"properties": {
				"jscs.enable": {
					"type": "boolean",
					"default": false,
					"description": "Control whether JSCS is enabled for JavaScript files or not."
				},
				"jscs.options": {
					"type": "object",
					"default": {},
					"description": "The jscs options object to provide args to the jscs command."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"vscode": "*"
	},
	"dependencies": {
		"vscode-languageclient": "next"
	}
}